{
  "name": "tinyai",
  "version": "0.1.0",
  "description": "A CLI tool for interacting with various LLM providers using Vercel AI SDK",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "tinyai": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "ai",
    "cli",
    "llm",
    "openai",
    "anthropic",
    "vercel"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.36.3",
    "@cliffy/command": "npm:@jsr/cliffy__command@1.0.0-rc.7",
    "ai": "^4.1.27",
    "nanoid": "^5.0.9",
    "openai": "^4.83.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
}
